{
  "compilerOptions": {
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@core/*": [
        "engine/*"
      ],
      "@common/*": [
        "engine/web/common/*"
      ],
      "@collect/*":[
        "engine/web/collect/*"
      ],
      
      "@accountManagement/*": [
        "engine/web/accountManagement/*"
      ],
      "@pages/*": [
        "pageFactory/pageRepository/*"
      ],
      "@objects/*": [
        "pageFactory/objectRepository/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@plugins/*": [
        "plugins/*"
      ]
    }
  }
}